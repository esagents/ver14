<odoo>
  <data>
    <record model="ir.ui.view" id="data_api.list">
      <field name="name">data_api list</field>
      <field name="model">data_api.data_api</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>



    <record model="ir.actions.act_window" id="data_api.action_window">
      <field name="name">data_apie window</field>
      <field name="res_model">data_api.data_api</field>
      <field name="view_mode">tree,form</field>
    </record>



    <record model="ir.actions.server" id="data_api.action_server">
      <field name="name">data_api server</field>
      <field name="model_id" ref="data_api_data_api"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <menuitem name="data_api" id="data_api.menu_root"/>

    <menuitem name="Menu 1" id="data_api.menu_1" parent="data_api.menu_root"/>
    <menuitem name="Menu 2" id="data_api.menu_2" parent="data_api.menu_root"/>

    <menuitem name="List" id="data_api.menu_1_list" parent="data_api.menu_1"
              action="data_api.action_window"/>
    <menuitem name="Server to list" id="data_apie" parent="data_api.menu_2"
              action="data_api.action_server"/>

  </data>
</odoo>